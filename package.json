{
	"name": "files-sync-action",
	"version": "0.0.0",
	"description": "Files sync action for Github",
	"keywords": [
		"actions",
		"node",
		"setup"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adrianjost/files-sync-action.git"
	},
	"license": "Apache 2.0",
	"author": "Adrian Jost",
	"main": "lib/index.js",
	"scripts": {
		"all": "npm run build && npm run format-check && npm run lint && npm test && npm run pack",
		"build": "tsc",
		"format": "prettier --write **/*.ts",
		"format-check": "prettier --check **/*.ts",
		"lint": "eslint src/**/*.ts",
		"lint-fix": "eslint --fix src/**/*.ts",
		"pack": "ncc build",
		"test": "jest",
		"dev": "npm run build && npm run pack && node dist/index"
	},
	"dependencies": {
		"@actions/core": "^1.2.0",
		"@octokit/plugin-retry": "^3.0.1",
		"@octokit/plugin-throttling": "^3.2.0",
		"@octokit/rest": "^17.1.0",
		"glob": "^7.1.6",
		"p-limit": "^2.3.0",
		"strip-ansi": "^6.0.0",
		"tweetsodium": "0.0.4"
	},
	"devDependencies": {
		"@octokit/fixtures": "^21.0.2",
		"@octokit/types": "^2.5.0",
		"@types/glob": "^7.1.1",
		"@types/jest": "^24.0.23",
		"@types/node": "^12.12.30",
		"@types/tmp": "^0.1.0",
		"@typescript-eslint/parser": "^2.24.0",
		"@zeit/ncc": "^0.20.5",
		"eslint": "^5.16.0",
		"eslint-plugin-github": "^2.0.0",
		"eslint-plugin-jest": "^22.21.0",
		"jest": "^24.9.0",
		"jest-circus": "^24.9.0",
		"jest-environment-uint8array": "^1.0.0",
		"js-yaml": "^3.13.1",
		"nock": "^12.0.3",
		"prettier": "^1.19.1",
		"ts-jest": "^24.2.0",
		"typescript": "^3.6.4"
	}
}
